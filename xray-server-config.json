{
    "log": {
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 8443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "USER-ID-1",
                        "email": "iPhone",
                        "flow": "xtls-rprx-vision"
                    },
                    {
                        "id": "USER-ID-2",
                        "email": "Mac",
                        "flow": "xtls-rprx-vision"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "show": false,
                    "dest": "www.apple.com:443",
                    "xver": 0,
                    "serverNames": [
                        "www.apple.com"
                    ],
                    "privateKey": "YOUR-PRIVATE-KEY",
                    "shortIds": [
                        "abcd1234"
                    ]
                },
                "sockopt": {
                    "tcpFastOpen": true,
                    "tcpNoDelay": true
                }
            }	
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ]
}
